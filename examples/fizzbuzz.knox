func main() void {
    fizzbuzz(300);
}

func fizzbuzz(n : int) void {
    var i : int = 1;
    while i <= n {
        if i%15 == 0 {
            stl.print("FizzBuzz");
        } else if i%3 == 0 {
            stl.print("Fizz");
        } else if i%5 == 0 {
            stl.print("Buzz");
        } else {
            stl.print("");
        }
        i = i + 1;
    }
}